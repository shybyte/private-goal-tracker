<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.menu {
			list-style: none;
			margin: 5px 0 20px;
			padding: 0;
			overflow: hidden;
		}
		
		.menu li {
			margin: 0 10px 0 0;
			padding: 2px;
			float: left;
			background: #f8f8f8;
		}
		
		.selected {
			font-weight: bold;
		}
		
	</ui:style>
	<ul class="{style.menu}" >
		<li class="{style.selected}"> Goal List</li>
		<li><a href="#options">Options</a></li>
	</ul>
</ui:UiBinder> 